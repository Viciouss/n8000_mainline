# /etc/inittab
#
# Copyright (C) 2001 Erik Andersen <andersen@codepoet.org>
#
# Note: BusyBox init doesn't support runlevels.  The runlevels field is
# completely ignored by BusyBox init. If you want runlevels, use
# sysvinit.
#
# Format for each entry: <id>:<runlevels>:<action>:<process>
#
# id        == tty to run on, or empty for /dev/console
# runlevels == ignored
# action    == one of sysinit, respawn, askfirst, wait, and once
# process   == program to run

# Startup the system
::sysinit:/bin/mount -t proc proc /proc
::sysinit:/bin/mount -o remount,rw /
::sysinit:/bin/mkdir -p /dev/pts /dev/shm
::sysinit:/bin/mount -a
::sysinit:/sbin/swapon -a
null::sysinit:/bin/ln -sf /proc/self/fd /dev/fd
null::sysinit:/bin/ln -sf /proc/self/fd/0 /dev/stdin
null::sysinit:/bin/ln -sf /proc/self/fd/1 /dev/stdout
null::sysinit:/bin/ln -sf /proc/self/fd/2 /dev/stderr
::sysinit:/bin/hostname -F /etc/hostname

# load kernel modules
::sysinit:/sbin/modprobe rfkill.ko
::sysinit:/sbin/modprobe brcmfmac.ko
::sysinit:/sbin/modprobe microchip.ko
::sysinit:/sbin/modprobe brcmutil.ko
::sysinit:/sbin/modprobe dm-crypt.ko
::sysinit:/sbin/modprobe bcma.ko
::sysinit:/sbin/modprobe cdc-wdm.ko
::sysinit:/sbin/modprobe cdc-acm.ko
::sysinit:/sbin/modprobe usblp.ko
::sysinit:/sbin/modprobe usbtmc.ko
::sysinit:/sbin/modprobe ssb.ko
::sysinit:/sbin/modprobe cpufreq_powersave.ko
::sysinit:/sbin/modprobe cpufreq_userspace.ko
::sysinit:/sbin/modprobe cpufreq_conservative.ko
::sysinit:/sbin/modprobe s5p-jpeg.ko
::sysinit:/sbin/modprobe s5p-mfc.ko
::sysinit:/sbin/modprobe exynos-gsc.ko
::sysinit:/sbin/modprobe s5p-cec.ko
::sysinit:/sbin/modprobe exynos-fimc-lite.ko
::sysinit:/sbin/modprobe s5p-fimc.ko
::sysinit:/sbin/modprobe exynos-fimc-is.ko
::sysinit:/sbin/modprobe s5p-csis.ko
::sysinit:/sbin/modprobe exynos4-is-common.ko
::sysinit:/sbin/modprobe vivid.ko
::sysinit:/sbin/modprobe v4l2-mem2mem.ko
::sysinit:/sbin/modprobe v4l2-fwnode.ko
::sysinit:/sbin/modprobe videodev.ko
::sysinit:/sbin/modprobe v4l2-common.ko
::sysinit:/sbin/modprobe v4l2-dv-timings.ko
::sysinit:/sbin/modprobe media.ko
::sysinit:/sbin/modprobe gspca_main.ko
::sysinit:/sbin/modprobe uvcvideo.ko
::sysinit:/sbin/modprobe v4l2-tpg.ko
::sysinit:/sbin/modprobe videobuf2-v4l2.ko
::sysinit:/sbin/modprobe videobuf2-common.ko
::sysinit:/sbin/modprobe videobuf2-memops.ko
::sysinit:/sbin/modprobe videobuf2-dma-contig.ko
::sysinit:/sbin/modprobe videobuf2-vmalloc.ko
::sysinit:/sbin/modprobe algif_skcipher.ko
::sysinit:/sbin/modprobe algif_rng.ko
::sysinit:/sbin/modprobe lz4.ko
::sysinit:/sbin/modprobe lzo-rle.ko
::sysinit:/sbin/modprobe ecc.ko
::sysinit:/sbin/modprobe algif_aead.ko
::sysinit:/sbin/modprobe lzo.ko
::sysinit:/sbin/modprobe algif_hash.ko
::sysinit:/sbin/modprobe chacha_generic.ko
::sysinit:/sbin/modprobe dh_generic.ko
::sysinit:/sbin/modprobe crypto_simd.ko
::sysinit:/sbin/modprobe ecdh_generic.ko
::sysinit:/sbin/modprobe lrw.ko
::sysinit:/sbin/modprobe salsa20_generic.ko
::sysinit:/sbin/modprobe xts.ko
::sysinit:/sbin/modprobe af_alg.ko
::sysinit:/sbin/modprobe sha3_generic.ko
::sysinit:/sbin/modprobe crypto_user.ko
::sysinit:/sbin/modprobe cryptd.ko
::sysinit:/sbin/modprobe sha512_generic.ko
::sysinit:/sbin/modprobe chacha-neon.ko
::sysinit:/sbin/modprobe sha512-arm.ko
::sysinit:/sbin/modprobe aes-arm-bs.ko
::sysinit:/sbin/modprobe sha1-arm.ko
::sysinit:/sbin/modprobe sha1-arm-neon.ko
::sysinit:/sbin/modprobe sha256-arm.ko
::sysinit:/sbin/modprobe lzo_decompress.ko
::sysinit:/sbin/modprobe lzo_compress.ko
::sysinit:/sbin/modprobe lz4_decompress.ko
::sysinit:/sbin/modprobe lz4_compress.ko
::sysinit:/sbin/modprobe cordic.ko

# now run any rc scripts
::sysinit:/etc/init.d/rcS

# Put a getty on the serial port
ttySAC2::respawn:/sbin/getty -L  ttySAC2 115200 vt100 # GENERIC_SERIAL

# Stuff to do for the 3-finger salute
#::ctrlaltdel:/sbin/reboot

# Stuff to do before rebooting
::shutdown:/etc/init.d/rcK
::shutdown:/sbin/swapoff -a
::shutdown:/bin/umount -a -r
